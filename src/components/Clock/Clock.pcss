.clockWrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  min-width: 90px;
  cursor: default;
  .clockTime {
    display: flex;
    flex-direction: column;
    align-items: flex-end;
    color: #191919;
    font-size: 14px;
    font-weight: 400;
    line-height: 18px;
    .mscTime {
      color: #7F7F7F;
    }
  }
}