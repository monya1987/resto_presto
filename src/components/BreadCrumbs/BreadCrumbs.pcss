.breadcrumbsWrapper {
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  .breadcrumbs {
    font-size: 14px;
    font-weight: 600;
    font-style: normal;
    font-stretch: normal;
    line-height: 1.71;
    letter-spacing: normal;
    color: #7f7f7f;
    text-decoration: none;
  }
  .breadcrumbsLast {
    color: #000;
    cursor: default;
  }
}